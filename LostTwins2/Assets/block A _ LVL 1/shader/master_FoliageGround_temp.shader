			float2 uv_transparent = i.uv_texcoord * _transparent_ST.xy + _transparent_ST.zw;
			float4 tex2DNode19 = tex2D( _transparent, uv_transparent );
			o.Alpha = tex2DNode19.r;
			#if UNITY_PASS_SHADOWCASTER
			clip( tex2DNode19.r - _Cutoff );
			#endif
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf Lambert keepalpha fullforwardshadows exclude_path:deferred 

		ENDCG
